{
  "manifest_version": 2,
  "name": "midori-tab",
  "description": "",
  "version": "1.0.0",
  "author": "Im-Jota",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "https://feeds.bbci.co.uk/*",
    "https://astian.org/*",
    "https://blog.mozilla.org/*",
    "https://github.blog/*",
    "https://api.allorigins.win/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "action": {
    "default_icon": {
      "16": "icons/128.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
