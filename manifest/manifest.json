{
  "manifest_version": 3,
  "name": "midori-tab",
  "description": "A lightweight, fast, and productivity-oriented new tab.",
  "version": "1.0.5",
  "author": "Im-Jota",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "identity",
    "webRequest",
    "webNavigation",
    "tabs"
  ],
  "host_permissions": [
    "https://api.rss2json.com/*",
    "https://api.unsplash.com/*"
  ],
  "action": {
    "default_icon": {
      "16": "icons/128.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "type": "module"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "midoritabs@astian.org",
      "strict_min_version": "114.0"
    }
  }
}