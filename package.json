    "@fontsource/lexend-deca": "4.5.14",
    "@fontsource/montserrat": "4.5.14",
    "@mui/material": "5.11.13",
    "@sentry/react": "^7.42.0",
    "embla-carousel-autoplay": "^7.1.0",
    "embla-carousel-react": "^7.1.0",
    "fast-blurhash": "^1.1.2",
    "image-conversion": "^2.1.1",
    "react": "^18.2.0",
    "react-clock": "4.1.0",
    "react-color-gradient-picker": "0.1.2",
