{
  "manifest_version": 3,
  "name": "midori-tab",
  "description": "",
  "version": "1.0.0",
  "author": "Im-Jota",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
