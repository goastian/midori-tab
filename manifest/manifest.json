{
  "manifest_version": 3,
  "name": "midori-tab",
  "description": "",
  "version": "1.0.6",
  "author": "Im-Jota",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "identity",
    "webRequest",
    "webNavigation",
    "tabs",
    "history",
    "bookmarks",
    "scripting"
  ],
  "host_permissions": [
    "https://api.rss2json.com/*",
    "https://api.unsplash.com/*",
    "<all_urls>"
  ],
  "commands": {
    "toggle-command-palette": {
      "suggested_key": {
        "default": "Ctrl+Alt+Space",
        "mac": "Command+Alt+Space"
      },
      "description": "Abrir paleta de comandos"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content-script.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "midoritabs@astian.org",
      "strict_min_version": "109.0"
    }
  }
}