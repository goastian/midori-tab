    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.3",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.4",
    "sass": "^1.59.2",
